  {{#if expenses.length}}
  <div class="col-md-6 project-list">
    <h3>Active Expenses:</h3>

    {{#each expense as |expense|}}
    <div class="row mb-2">
      <div class="col-md-8">
        <h4><a href="/newexpense/{{newexpense.id}}">{{newexpense.name}}</a></h4>
      </div>
      <div class="col-md-4">
        <button class="btn btn-sm btn-warning" data-id="{{project.id}}">REMOVE</button>
      </div>
    </div>
    {{/each}}
  </div>
  {{/if}}
</div>

<script src="../public/js/profile.js"></script>
